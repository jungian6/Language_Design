FUN fibonacci(n)
    IF n <= 1 THEN
        RETURN n;
    ELSE
        RETURN fibonacci(n-1) + fibonacci(n-2);
    END
END

FUN generateFibonacciSequence(limit)
    VAR sequence = [];
    FOR i = 0 TO limit THEN
        APPEND(sequence, fibonacci(i));
    END
    RETURN sequence;
END

VAR fibSequence = generateFibonacciSequence(10);
PRINT("Fibonacci sequence up to 10th term: " + fibSequence);
